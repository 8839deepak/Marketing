@model IEnumerable<Marketing.Models.NewOrders>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Marketing.Models
@{ List<Ragistation> listragistation = new Ragistation().GetAll();
    //NewOrders ObjOrder = new NewOrders();
    //List<NewOrders> listnewOrder = ObjOrder.GetAll();
    List<OrderItems> listorderitems = OrderItems.GetAll();
    List<Orders> listorder = new Orders().GetAll();
    //var dateTime = DateTime.Today;
    List<NewOrders> listneworder = new NewOrders().GetAll();

    ////listneworder = listneworder.FindAll(x => x.RagisID == ee.RagisID && x.Create_Date == ee.Create_Date);
    //foreach (var dd in listneworder)
    //{
    //    listneworder = listneworder.FindAll(x => x.Create_Date == dateTime && x.RagisID == dd.RagisID);
    //}

}
<div class="card">
    <div class="card-header">
        <h5 class="text-center text-danger"><b>Order List</b></h5>
    </div>
    <div class="card-body">
        @foreach (var item in Model)
        {
            var neworder = new NewOrders().GetAll();
            //var neworder1 = new NewOrders().GetAll();
            //var neworder2 = new NewOrders().GetAll();
            var bd = listragistation.FindAll(x => x.RagisID == item.RagisID);
            listragistation = bd.FindAll(x => x.RagisID == item.RagisID);
            var rg = neworder.FindAll(x => x.RagisID == item.RagisID);
            neworder = rg.FindAll(x => x.RagisID == item.RagisID);
            <div class="row w-100">
                <div class="row w-100 pt-3 pb-2" style="border:groove;border-color:#a8f56d">
                    <div class=" col-sm-4">
                        @Html.Label("Name :-")
                        @{
                            Ragistation ObjRagistation = listragistation.Find(x => x.RagisID == item.RagisID);
                            @ObjRagistation.Name;
                        }
                    </div>
                    <div class="col-sm-4">
                        @Html.Label("Address")
                        @{
                            Ragistation ObjRagistatio = listragistation.Find(x => x.RagisID == item.RagisID);
                            @ObjRagistatio.Address;
                        }
                    </div>
                    <div class="col-sm-4">
                        @Html.Label("Mobile")
                        @{
                            Ragistation ObjRagistatio1 = listragistation.Find(x => x.RagisID == item.RagisID);
                            @ObjRagistatio1.Mobile;
                        }
                    </div>
                </div>
                <div class="row w-100">
                    <div class="col-sm-2">
                        @Html.Label("Product Name")<br />
                        <ul>
                            @foreach (var jj in neworder)
                            {
                                <li>
                                    @jj.ProductName
                                </li>
                            }
                        </ul>
                    </div>
                    <div class="col-sm-2">
                        @Html.Label("Product Prize")<br />
                        <ul>
                            @foreach (var jj in neworder)
                            {
                                <li>
                                    @jj.ProductPrize
                                </li>
                            }
                        </ul>
                    </div>
                    <div class="col-sm-2">
                        @Html.Label("Quantity")<br />
                        <ul>
                            @foreach (var jj in neworder)
                            {
                                <li>
                                    @jj.Qty
                                </li>
                            }
                        </ul>
                    </div>
                    <div class="col-sm-2">
                        @Html.Label("Total Quantity")<br />
                        <ul>
                            @foreach (var jj in neworder)
                            {
                                <li>
                                    @jj.TotalQty
                                </li>
                            }
                        </ul>
                    </div>
                    <div class="col-sm-2">
                        @Html.Label("Total Prize")<br />
                        <ul>

                            <li>
                                @item.PrizeTotal
                            </li>

                        </ul>
                    </div>
                    <div class="col-sm-2">
                        @Html.Label("Order Date")<br />
                        <ul>

                            <li>
                                @item.Create_Date
                            </li>

                        </ul>
                    </div>
                </div>


            </div>

        }
    </div>

</div>






